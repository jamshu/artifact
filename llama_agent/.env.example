# Gemini API Configuration
GEMINI_API_KEY=your_gemini_api_key_here

# Odoo Development Settings
ODOO_ADDONS_PATH=/path/to/your/odoo/addons
ODOO_VERSION=16.0

# Agent Configuration
AGENT_MODE=development
AUTO_APPROVE_CHANGES=false
BACKUP_FILES=true

# Web UI Configuration
UI_HOST=localhost
UI_PORT=5000
UI_DEBUG=true