<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="approve_stock_adjustment_barcode_group" model="res.groups">
        <field name="name">Approve Stock Adjustment Barcode</field>
        <field name="users" eval="[Command.link(ref('base.user_root')), Command.link(ref('base.user_admin'))]"/>
    </record>

    <record id="stock_adjustment_barcode_rule" model="ir.rule" >
        <field name="name">Stock Adjustment multi-company</field>
        <field name="model_id" search="[('model','=','stock.adjustment.barcode')]" model="ir.model"/>
        <field name="domain_force">[('company_id', 'in', company_ids)]</field>
    </record>

</odoo>
